<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon />
    <StartupObject />
    <Version>0.0.2</Version>
    <Authors>TonesNotes</Authors>
    <Company>kzbsv.org</Company>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/kzbsv/KZJ</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="ButtonMenuForm.cs" />
    <Compile Update="ButtonMenuForm.designer.cs">
      <DependentUpon>ButtonMenuForm.cs</DependentUpon>
    </Compile>
    <Compile Update="DynamicDialog.cs" />
    <Compile Update="KzPropertiesGridDialog.cs" />
    <Compile Update="KzPropertiesGridDialog.designer.cs">
      <DependentUpon>KzPropertiesGridDialog.cs</DependentUpon>
    </Compile>
    <Compile Update="LayoutConfirmDeleteForm.cs" />
    <Compile Update="LayoutSaveAsForm.cs" />
    <Compile Update="LayoutSaveAsForm.Designer.cs">
      <DependentUpon>LayoutSaveAsForm.cs</DependentUpon>
    </Compile>
    <Compile Update="LogControl.cs" />
    <Compile Update="NewTabLayoutForm.cs" />
    <Compile Update="OkCancelControl.cs" />
    <Compile Update="OkCancelControl.Designer.cs">
      <DependentUpon>OkCancelControl.cs</DependentUpon>
    </Compile>
    <Compile Update="UiLayoutTabControl.cs" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="LayoutConfirmDeleteForm.resx">
      <DependentUpon>LayoutConfirmDeleteForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="NewTabLayoutForm.resx">
      <DependentUpon>NewTabLayoutForm.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>